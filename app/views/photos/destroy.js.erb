$('#photos').html("<%= j render 'photos_list' %>")
<% if @car.photos.blank? %>
$('#photo_check').hide();
<%end%>

<% if @car.active && !@car.price.blank? && !@car.listing_name.blank? && !@car.photos.blank? && !@car.location.blank? %>
$('#publish_button').attr('disabled', false)
<%else%>
$('#publish_button').attr('disabled', true)
<%end%>